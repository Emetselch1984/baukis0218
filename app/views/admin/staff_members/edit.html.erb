<% @title = "職員情報の編集" %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_with model: @staff_member, url: [ :admin, @staff_member ],local: true do |f| %>
    <%= render "form", f: f %>
    <div class="buttons">
      <%= f.submit "更新する" %>
      <%= link_to "キャンセル", :admin_staff_members %>
    </div>
  <% end %>
</div>
